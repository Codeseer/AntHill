extends ../layout

block content

  .row.center-align
    form(action="/projects", method="POST")
      label(for='projectname') Project Name
      input(type='text', name='projectname', id='projectname', maxLength='255')
      
      label(for='mode') COCOMO Mode
      select(name='mode', id='mode')
        option(value='organic') Organic
        option(value='semi-detached') Semi-detached
        option(value='embedded') Embedded
      
      h2 Project Attributes
